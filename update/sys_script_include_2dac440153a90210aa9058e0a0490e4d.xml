        <name>set_fields_read_only</name>
        <script><![CDATA[var FieldReadOnlyUtil = Class.create();
FieldReadOnlyUtil.prototype = {
    initialize: function () {},
    setFieldsReadOnly: function () {
        var fields = g_form.getEditableFields();
